config_version=5

[application]

config/name="VitaVehicle - Godot 4.2"
config/description="Realistic Vehicle Simulation for Godot 4.2"
run/main_scene="res://world.tscn"
config/features=PackedStringArray("4.2")
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 1)
boot_splash/image="res://logo.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://vlogo.png"

[autoload]

VitaVehicleSimulation="*res://MAIN/vitavehicle.gd"
misc_graphics_settings="*res://MISC/autoload/graphics.gd"

[debug]

